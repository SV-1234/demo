e: \"合肥市\", value: 0.003493311 },\n            { name: \"淮北市\", value: 0.002775681 },\n            { name: \"淮南市\", value: 0.00310308 },\n            { name: \"黄山市\", value: 0.005104356 },\n            { name: \"六安市\", value: 0.003606148 },\n            { name: \"马鞍山市\", value: 0.0035555 },\n            { name: \"宿州市\", value: 0.002646484 },\n            { name: \"铜陵市\", value: 0.003548752 },\n            { name: \"芜湖市\", value: 0.003519977 },\n            { name: \"宣城市\", value: 0.004052618 },\n          ],\n        },\n        {\n          name: \"北京市\",\n          cities: [{ name: \"北京市\", value: 0.001686256 }],\n        },\n        {\n          name: \"福建省\",\n          cities: [\n            { name: \"福州市\", value: 0.004367732 },\n            { name: \"龙岩市\", value: 0.004063561 },\n            { name: \"南平市\", value: 0.004450702 },\n            { name: \"宁德市\", value: 0.00489777 },\n            { name: \"莆田市\", value: 0.003911191 },\n            { name: \"泉州市\", value: 0.004004511 },\n            { name: \"三明市\", value: 0.003965118 },\n            { name: \"厦门市\", value: 0.003650881 },\n            { name: \"漳州市\", value: 0.003940893 },\n          ],\n        },\n      ],\n    };\n  },\n  mounted() {\n    this.initChart();\n  },\n  methods: {\n    initChart() {\n      this.chart = echarts.init(this.$refs.radarChart);\n      this.updateChart();\n    },\n    updateChart() {\n      const province = this.radarData.find(\n        (p) => p.name === this.selectedProvince\n      );\n      const option = {\n        title: {\n          text: province.name,\n        },\n        tooltip: {},\n        legend: {\n          data: [province.name],\n        },\n        radar: {\n          indicator: province.cities.map((city) => ({\n            name: city.name,\n            max: 0.01,\n          })),\n        },\n        series: [\n          {\n            name: province.name,\n            type: \"radar\",\n            data: [\n              {\n                value: province.cities.map((city) => city.value),\n                name: province.name,\n              },\n            ],\n          },\n        ],\n      };\n      this.chart.setOption(option);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.chart-container {\n  width: 100%;\n  height: 400px; /* 调整高度以适应内容 */\n  margin-top: 40px; /* 调整距离以向下移动 */\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n</style>\n"]}]}