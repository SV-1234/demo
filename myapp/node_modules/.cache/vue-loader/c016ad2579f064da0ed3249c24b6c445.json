v>\n    <h2>平均用水排行</h2>\n    <div class=\"chart\" id=\"myEchartsTwo\">图表的容器</div>\n  </div>\n</template>\n\n<script>\nimport { inject, onMounted, reactive } from \"vue\";\nexport default {\n  setup() {\n    let $echarts = inject(\"echarts\");\n    let $http = inject(\"axios\");\n\n    let data = reactive({});\n\n    async function getState() {\n      data = await $http({ url: \"/two/data\" });\n    }\n\n    onMounted(() => {\n      getState().then(() => {\n        console.log(\"折线图\", data);\n\n        let myChart = $echarts.init(document.getElementById(\"myEchartsTwo\"));\n        myChart.setOption({\n          tooltip: {\n            trigger: \"axis\",\n            axisPointer: {\n              type: \"cross\",\n              label: {\n                backgroundColor: \"#e6b600\",\n              },\n            },\n          },\n          legend: {\n            textStyle: {\n              color: \"#fff\", // 将图例字体颜色设置为白色\n            },\n            data: [\"江苏\", \"安徽\", \"江西\", \"广东\", \"黑龙江\"],\n          },\n          grid: {\n            left: \"1%\",\n            right: \"4%\",\n            bottom: \"3%\",\n            containLabel: true,\n          },\n          xAxis: {\n            type: \"category\",\n            boundaryGap: false,\n            data: data.data.chartTwo.chartData.day,\n            axisLine: {\n              lineStyle: {\n                color: \"#fff\",\n              },\n            },\n          },\n          yAxis: {\n            type: \"value\",\n            axisLine: {\n              lineStyle: {\n                color: \"#fff\",\n              },\n            },\n          },\n          series: [\n            {\n              name: \"江苏\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.江苏,\n              //  smooth: true,\n              showSymbol: false,\n              lineStyle: {\n                color: \"rgb(128, 255, 165)\",\n              },\n              emphasis: {\n                focus: \"series\",\n              },\n            },\n            {\n              name: \"安徽\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.安徽,\n              // smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(0, 221, 255)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n            {\n              name: \"江西\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.江西,\n              //   smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(55, 162, 255)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n            {\n              name: \"广东\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.广东,\n              //  smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(255, 0, 135)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n            {\n              name: \"黑龙江\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.黑龙江,\n              // smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(255, 191, 0)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n          ],\n        });\n      });\n    });\n    return {\n      getState,\n      data,\n    };\n  },\n};\n</script>\n\n<style>\nh2 {\n  height: 0.6rem;\n  color: #fff;\n  line-height: 0.6rem;\n  font-size: 0.25rem;\n  text-align: center;\n}\n.chart {\n  height: 4.5rem;\n}\n</style>\n"]}]}