rts\");\n    let $http = inject(\"axios\");\n\n    let data = reactive({});\n\n    async function getState() {\n      data = await $http({ url: \"/three/data\" });\n    }\n\n    onMounted(() => {\n      getState().then(() => {\n        console.log(\"饼状图\", data);\n\n        let myChart = $echarts.init(document.getElementById(\"myEcharts\"));\n        myChart.setOption({\n          legend: {\n            top: \"bottom\",\n            textStyle: {\n              color: \"#fff\", // 修改图例 name 的字体颜色为白色\n            },\n          },\n          tooltip: {\n            show: true,\n          },\n          series: [\n            {\n              type: \"pie\",\n              data: data.data.chartThree.chartData,\n              radius: [5, 70],\n              center: [\"50%\", \"45%\"],\n              roseType: \"area\",\n              itemStyle: {\n                borderRadius: 10,\n              },\n              label: {\n                textStyle: {\n                  color: \"#fff\", // 修改扇区标签文字颜色为白色\n                },\n              },\n            },\n          ],\n        });\n      });\n    });\n    return {\n      getState,\n      data,\n    };\n  },\n};\n</script>\n\n<style>\nh2 {\n  height: 0.6rem;\n  color: #fff;\n  line-height: 0.6rem;\n  font-size: 0.25rem;\n  text-align: center;\n}\n.chart {\n  height: 2.25rem;\n}\n</style>"]}]}