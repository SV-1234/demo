 inject(\"echarts\");\n    let $http = inject(\"axios\");\n\n    let data = reactive({});\n\n    async function getState() {\n      data = await $http({ url: \"/four/data\" });\n    }\n\n    onMounted(() => {\n      getState().then(() => {\n        console.log(\"雷达图\", data);\n\n        let chartData = data.data.chartFour.chartData.num;\n        let maxVal = Math.max(...Object.values(chartData));\n\n        let indicators = Object.keys(chartData).map((key) => ({\n          name: key,\n          max: maxVal,\n        }));\n\n        let myChart = $echarts.init(document.getElementById(\"myEchartsFour\"));\n        myChart.setOption({\n          radar: {\n            indicator: indicators,\n            axisLine: {\n              lineStyle: {\n                color: \"#fff\",\n              },\n            },\n            splitLine: {\n              lineStyle: {\n                color: \"#fff\",\n              },\n            },\n          },\n          tooltip: {\n            trigger: \"item\",\n          },\n          series: [\n            {\n              name: \"统计\",\n              type: \"radar\",\n              data: [\n                {\n                  value: Object.values(chartData),\n                  name: \"统计\",\n                },\n              ],\n              areaStyle: {},\n            },\n          ],\n        });\n      });\n    });\n    return {\n      getState,\n      data,\n    };\n  },\n};\n</script>\n\n<style>\nh2 {\n  height: 0.6rem;\n  color: #fff;\n  line-height: 0.6rem;\n  font-size: 0.25rem;\n  text-align: center;\n}\n.chart {\n  height: 4.5rem;\n}\n</style>\n"]}]}