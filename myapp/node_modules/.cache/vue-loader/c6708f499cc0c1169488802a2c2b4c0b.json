    name: \"安徽\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.安徽,\n              // smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(0, 221, 255)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n            {\n              name: \"江西\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.江西,\n              //   smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(55, 162, 255)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n            {\n              name: \"广东\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.广东,\n              //  smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(255, 0, 135)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n            {\n              name: \"黑龙江\",\n              type: \"line\",\n              data: data.data.chartTwo.chartData.num.黑龙江,\n              // smooth: true, //折线图平滑效果 变成曲线图\n              showSymbol: false, //   隐藏所有数据点\n              lineStyle: {\n                color: \"rgb(255, 191, 0)\",\n              },\n              emphasis: {\n                focus: \"series\", //只显示选中的内容高亮\n              },\n            },\n          ],\n        });\n      });\n    });\n    return {\n      getState,\n      data,\n    };\n  },\n};\n</script>\n\n<style>\nh2 {\n  height: 0.6rem;\n  color: #fff;\n  line-height: 0.6rem;\n  font-size: 0.25rem;\n  text-align: center;\n}\n.chart {\n  height: 4.5rem;\n}\n</style>\n"]}]}